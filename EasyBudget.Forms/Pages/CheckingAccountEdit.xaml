<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="EasyBudget.Forms.Pages.CheckingAccountEdit">

    <ContentPage.Padding>
        <OnPlatform x:TypeArguments="Thickness">
            <On Platform="iOS" Value="10,40,0,0" />
            <On Platform="Android" Value="10,0,0,0" />
        </OnPlatform>
    </ContentPage.Padding> 
    
    <ContentPage.Content>
        <StackLayout Spacing="10">
                <StackLayout VerticalOptions="FillAndExpand">
                    
                </StackLayout>
                <StackLayout Orientation="Horizontal" HorizontalOptions="CenterAndExpand">
                    <Button x:Name="SaveEdits" Text="Save" Clicked="OnSaveClicked"></Button>
                    <Button x:Name="CancelEdits" Text="Cancel" Clicked="OnCancelClicked"></Button>
                </StackLayout>

            </StackLayout>
	</ContentPage.Content>
    
</ContentPage>
